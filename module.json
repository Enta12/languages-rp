{
    "id": "languages-rp-fork",
    "title": "languages-rp-fork",
    "description": "Add languages to character and let them decrypt texts",
    "version": "0.0.1.2",
    "compatibility": {
        "minimum": "12",
        "verified": "13"
    },
    "authors": [
        {
            "name": "Enta12",
            "discord": "Enta12"
        }
    ],
    "languages": [
        {
            "lang": "fr",
            "name": "Français",
            "path": "lang/fr.json",
            "flags": {}
        },
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json",
            "flags": {}
        },
        {
            "lang": "ru",
            "name": "Russian",
            "path": "lang/ru.json",
            "flags": {}
        },
        {
            "lang": "pt-BR",
            "name": "Português (Brasil)",
            "path": "lang/pt-BR.json",
            "flags": {}
        },
        {
            "lang": "pl",
            "name": "Polski",
            "path": "lang/pl.json",
            "flags": {}
        },
        {
            "lang": "cn",
            "name": "Simplified Chinese",
            "path": "lang/cn.json",
            "flags": {}
        }
    ],
    "esmodules": [
        "./scripts/main.js",
        "./scripts/settings.js"
    ],
    "styles": [
        "./styles/menu-config.css"
    ],
    "flags": {
        "hotReload": {
            "extensions": [
                "json",
                "js"
            ],
            "paths": [
                "lang/",
                "scripts/"
            ]
        }
    }
}